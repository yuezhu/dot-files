[core]
	editor = emacs --no-window-system --quick
	pager = less --RAW-CONTROL-CHARS
	fileMode = true
	excludesfile = ~/.gitignore_global
[user]
	name = Yue Zhu
	useConfigOnly = true
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[push]
	default = simple
[rebase]
	autosquash = true
[remote]
	pushDefault = origin
[fetch]
	prune = true
	pruneTags = true
[pull]
	ff = only
[url "git@github.com:"]
	insteadOf = git://github.com/
	insteadOf = https://github.com/
